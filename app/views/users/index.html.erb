<h2>Users Index</h2>

<%= link_to 'Sign Up', new_user_path %>

<ul>
  <hr>
  <% @users.each do |user| %>
    <li><p>Username: <%= link_to "#{user.username}", user_path(user.id) %><p></li>
    <p>ID: <%= user.id %></p>
    <p>First Name: <%= user.first_name.capitalize %></p>
    <p>Last Name: <%= user.last_name.capitalize %></p>
    <p>Email: <%= user.email %></p>
    <hr>
  <% end %>
</ul>
